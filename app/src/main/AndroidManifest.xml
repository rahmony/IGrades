    package="net.rahmony.igrades">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_a"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".CourseActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".NewCourseActivity"></activity>
        <activity android:name=".GradeActivity"></activity>
        <activity android:name=".NewGradeActivity"></activity>
    </application>

</manifest>